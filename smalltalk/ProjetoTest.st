t methodsFor: 'testing' stamp: 'DEFB 10/22/2012 22:21'!testListaTarefa	self assert: (projeto listaTarefa isKindOf: Collection).	self assert: (projeto listaTarefa isEmpty).! !!ProjetoTest methodsFor: 'testing' stamp: 'DEFB 10/18/2012 15:37'!testNew	self should: [Projeto new] raise: Error.! !!ProjetoTest methodsFor: 'testing' stamp: 'DEFB 10/18/2012 15:40'!testNewNomeDescricao	self assert: (projeto isMemberOf: Projeto).	self assert: umNome equals: (projeto nome).	self assert: umaDescricao equals: (projeto descricao).! !!ProjetoTest methodsFor: 'testing' stamp: 'DEFB 10/18/2012 14:46'!testNome	| umNome |	umNome := 'Mensalzinho'.	self assert: umNome equals: (projeto nome: umNome).	self assert: umNome equals: (projeto nome).! !!ProjetoTest methodsFor: 'testing' stamp: 'DEFB 10/18/2012 15:08'!testUrl	| umaUrl |	umaUrl := 'http://www.bento.fot.br/' asUrl.	self assert: umaUrl equals: (projeto url: umaUrl).	self assert: umaUrl equals: (projeto url).! !